            <kind>1</kind>
            <string>///check area
players_in_area = global.p1_in_area 
                + global.p2_in_area
                + global.p3_in_area
                + global.p4_in_area
                + global.p5_in_area
                + global.p6_in_area
                + global.p7_in_area
                + global.p8_in_area;
//====Close wall                
if(players_in_area == global.num_players - global.dead_players){    //if all alive players are inside
    closed = true;
}else{
    alarm[0] = room_speed * 5;
}
//====check for mobs
if(place_meeting(x,y,obj_mob_father)){
    mobs = true;
    alarm[0] = room_speed * 5;
}else{
    mobs = false;
}
//====Start round 1
if(closed &amp;&amp; !round_1 &amp;&amp; !round_2 &amp;&amp; !round_3 &amp;&amp; !round_4 &amp;&amp; !round_5 &amp;&amp; cooldown &amp;&amp; !door){
    cooldown = false;
    round_1 = true;
}
if(round_1 &amp;&amp; !started){    //spawn round 1
    started = true;
    instance_create(x-400, y -100, obj_mob_slime_big);
    instance_create(x+400, y -100, obj_mob_slime_big);
    instance_create(x,y,obj_mob_slime_big);
    
    instance_create(x-400, y -100, obj_mob_flypod_slime);
    instance_create(x+400, y -100, obj_mob_flypod_slime);
}

//====Start round 2
if(round_1 &amp;&amp; !mobs &amp;&amp; cooldown){
    cooldown = false;
    round_1 = false;
    round_2 = true;
    started = false;
}
if(round_2 &amp;&amp; !started){    //spawn round 2
    started = true;
    instance_create(x-400, y -100, obj_mob_knight);
    instance_create(x+400, y -100, obj_mob_knight);
    instance_create(x,y,obj_mob_slime_big);
    
    instance_create(x-100, y -100, obj_mob_flypod_slime);
    instance_create(x+100, y -100, obj_mob_flypod_slime);
}

//====Start round 3
if(round_2 &amp;&amp; !mobs &amp;&amp; cooldown){
    cooldown = false;
    round_2 = false;
    round_3 = true;
    started = false;
}
if(round_3 &amp;&amp; !started){    //spawn round 3
    started = true;
    instance_create(x-400, y -100, obj_mob_golem);
    instance_create(x+400, y -100, obj_mob_golem);
    instance_create(x,y,obj_mob_knight);
    
    instance_create(x-100, y -100, obj_mob_flypod_slime);
    instance_create(x+100, y -100, obj_mob_flypod_slime);
}

//====spawn door

if(round_3 &amp;&amp; !mobs &amp;&amp; cooldown){
    door = true;
    round_3 = false;
}
